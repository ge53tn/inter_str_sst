FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    Inlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    Outlet
    {
        type            zeroGradient;
    }

    Wall
    {
        type            zeroGradient;
    }

    Open
    {
        type            zeroGradient;
    }


}

